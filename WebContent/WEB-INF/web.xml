<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>javaweb-exercise-02</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <request-character-encoding>UTF-8</request-character-encoding>
  <response-character-encoding>UTF-8</response-character-encoding>
  <servlet>
	<servlet-name>registerSLServlet</servlet-name>
	<servlet-class>web.test.servlet.RegisterSLServlet</servlet-class>
	<init-param>
		<param-name>Name</param-name>
		<param-value>188</param-value>
	</init-param>
	<init-param>
		<param-name>Height</param-name>
		<param-value>Justin</param-value>
	</init-param>
	<load-on-startup>1</load-on-startup>
</servlet>
<servlet-mapping>
	<servlet-name>registerSLServlet</servlet-name>
	<url-pattern>/registerSL</url-pattern>
	<url-pattern>/REGISTER_SL</url-pattern>
</servlet-mapping>
</web-app>

